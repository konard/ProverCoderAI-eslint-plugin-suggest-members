{
  "name": "effect-template-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo workspace for effect-template",
  "packageManager": "pnpm@10.28.2",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm --filter @prover-coder-ai/eslint-plugin-suggest-members build",
    "check": "pnpm --filter @prover-coder-ai/eslint-plugin-suggest-members check",
    "changeset": "changeset",
    "changeset-publish": "if [ -z \"$NPM_TOKEN\" ]; then echo \"Skipping publish: NPM_TOKEN is not set\"; exit 0; fi; changeset publish",
    "changeset-version": "changeset version",
    "lint": "pnpm --filter @prover-coder-ai/eslint-plugin-suggest-members lint",
    "lint:tests": "pnpm --filter @effect-template/eslint-plugin-suggest-members lint:tests",
    "lint:effect": "pnpm --filter @effect-template/eslint-plugin-suggest-members lint:effect",
    "test": "pnpm --filter @prover-coder-ai/eslint-plugin-suggest-members test",
    "typecheck": "pnpm --filter @prover-coder-ai/eslint-plugin-suggest-members typecheck",
    "lint-app": "pnpm --filter @effect-template/app lint"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@ton-ai-core/vibecode-linter": "^1.0.11",
    "eslint": "^9.39.2",
    "install": "^0.13.0",
    "npm": "^11.9.0",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProverCoderAI/eslint-plugin-suggest-members.git"
  },
  "bugs": {
    "url": "https://github.com/ProverCoderAI/eslint-plugin-suggest-members/issues"
  },
  "homepage": "https://github.com/ProverCoderAI/eslint-plugin-suggest-members#readme",
  "license": "ISC",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild",
      "node-pty"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "msgpackr-extract",
      "unrs-resolver"
    ]
  }
}
